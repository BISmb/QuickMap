<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Company>Marty Barrett</Company>
    <Copyright>MIT</Copyright>
    <Description>A mapper to quickly get setup with mapping operations.</Description>
    <PackageProjectUrl>https://github.com/BISmb/QuickMap</PackageProjectUrl>
    <RepositoryUrl>https://github.com/BISmb/QuickMap</RepositoryUrl>
    <PackageTags>mapping, mapper, map</PackageTags>
    <AssemblyVersion>0.0.0.1</AssemblyVersion>
    <FileVersion>0.0.0.1</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

</Project>
